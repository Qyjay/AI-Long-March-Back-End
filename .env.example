APP_NAME=LongMarch Backend
LOG_LEVEL=INFO
CORS_ORIGINS=*

# MySQL 连接串示例：请修改为真实账号口令与库名
DATABASE_URL=mysql+pymysql://root:password@localhost:3306/app

# LLM 与嵌入模型（DashScope 兼容 OpenAI 接口）
LLM_MODEL=qwen-plus
DASHSCOPE_API_KEY=your_dashscope_api_key
OPENAI_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
EMBEDDING_MODEL=text-embedding-v4
OPENAI_API_KEY=your_openai_compatible_api_key

# RAG 文档与索引路径（Windows 示例）
RAG_DOCX_PATH=App\重走长征路.docx
RAG_JSON_PATH=data.json
FAISS_DIR=./faiss_index